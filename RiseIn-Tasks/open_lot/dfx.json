{
  "canisters": {
    "open_lot_backend": {
      "candid": "src/open_lot_backend/open_lot_backend.did",
      "package": "open_lot_backend",
      "type": "rust"
    },
    "open_lot_frontend": {
      "dependencies": [
        "open_lot_backend"
      ],
      "source": [
        "src/open_lot_frontend/dist"
      ],
      "type": "assets",
      "build": [
        "cd src/open_lot_frontend && npm install && npm run build"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}